<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Linked Data Server</display-name>
	<servlet>
		<servlet-name>DefaultServlet</servlet-name>
		<servlet-class>es.eurohelp.lod.LinkedDataServer</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>DefaultServlet</servlet-name>
		<url-pattern></url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<context-param>
    	<param-name>internal-uri-base</param-name>
    	<param-value>http://opendata.euskadi.eus/</param-value>
	</context-param>
	<context-param>
    	<param-name>sparql-endpoint-query-url</param-name>
    	<param-value>http://ckan.eurohelp.es:7200/repositories/opendata-euskadi?query=</param-value>
	</context-param>
</web-app>